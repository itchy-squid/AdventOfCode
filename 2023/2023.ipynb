        "#@title Day 1-1 Trebuchet?!\n",
        "from google.colab import files\n",
        "with open('2023-1-1.txt','r') as f:\n",
        "  lines = f.read().splitlines()\n",
        "\n",
        "digits = [[i for i in l if '0' <= i <= '9'] for l in lines]\n",
        "values = list(map(int, [d[0]+d[-1] for d in digits]))\n",
        "print (sum(values))"
      ]
